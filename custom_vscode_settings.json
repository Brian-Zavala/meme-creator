{
    "workbench.sideBar.location": "left",
    "workbench.editor.enablePreview": false,
    "breadcrumbs.enabled": true,
    "editor.minimap.enabled": false,
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "prettier.requireConfig": false,
    "prettier.resolveGlobalModules": true,
    "notebook.formatOnSave.enabled": true,
    "editor.fontSize": 21,
    "editor.tabSize": 2,
    "editor.linkedEditing": true,
    "editor.cursorBlinking": "expand",
    "editor.lineNumbers": "on",
    "editor.lineNumbersMinChars": 3,
    "editor.glyphMargin": true,
    "editor.folding": true,
    "editor.showFoldingControls": "mouseover",
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.scrollBeyondLastLine": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.occurrencesHighlight": "singleFile",
    "editor.selectionHighlight": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.indentation": true,
    "editor.renderLineHighlight": "all",
    "editor.cursorSurroundingLines": 5,
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "[javascript]": {
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "security.allowedUNCHosts": [
        "wsl.localhost",
        "wsl$"
    ],
    "settingsSync.ignoredExtensions": [
        "equinusocio.vsc-material-theme"
    ],
    "github.copilot.chat.languageContext.typescript.enabled": true,
    "github.copilot.chat.languageContext.inline.typescript.enabled": true,
    "github.copilot.chat.languageContext.fix.typescript.enabled": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "redhat.telemetry.enabled": true,
    "remote.autoForwardPortsSource": "hybrid",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.ignorecase": true,
    "vim.smartcase": true,
    "vim.timeout": 1000,
    "vim.showcmd": true,
    "vim.showmodename": true,
    "vim.startInInsertMode": false,
    "vim.changeWordIncludesWhitespace": false,
    "vim.mouseSelectionGoesIntoVisualMode": true,
    "vim.foldfix": true,
    "vim.substituteGlobalFlag": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.easymotion": true,
    "vim.camelCaseMotion.enable": true,
    "vim.surround": true,
    "vim.commentary": true,
    "vim.replaceWithRegister": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 200,
    "vim.cursorStylePerMode": {
        "normal": "block",
        "insert": "line",
        "visual": "block-outline",
        "visualline": "block-outline",
        "visualblock": "block-outline",
        "replace": "underline"
    },
    "vim.handleKeys": {
        "<C-h>": false,
        "<C-a>": false,
        "<C-f>": false,
        "<C-p>": false,
        "<C-b>": false,
        "<C-t>": false,
        "<C-w>": false,
        "<C-n>": false,
        "<C-r>": true,
        "<C-d>": true
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
